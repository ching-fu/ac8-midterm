<h1>index.html.erb</h1>
<table>
  <thead>
    <tr>
    <th>time</th><th>topic</th>
    </tr> 
  </thead>
  <tbody>
  <%@posts.each do |p|%>
  <tr>
    <td>
      <%= p.updated_at%>
    </td>  
    <td>
      <%= p.topic%>
    </td>
	 <td>  	
	   <%= link_to "show", post_path(p) %>
	 </td>
	 <td>
    <%= link_to "edit", edit_post_path(p) %>
   </td>
   <td>
    <%= link_to "destroy", post_path(p), :method=>"delete" %>
   </td>
  </tr>
<%end%>
</tbody>
</table>
<%= paginate @posts %>
<%= link_to "new", new_post_path() %>
